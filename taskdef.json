{
    "executionRoleArn": "arn:aws:iam::683745400026:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "devops-app",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "devops-fargate-task"
}